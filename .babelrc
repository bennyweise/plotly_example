{
    "presets": ["es2015", "react", "stage-2"],
    "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread"
    ],
    "env": {
        "development": {
            "presets": ["es2015", "react", "stage-2"],
            "plugins": [
                "transform-class-properties",
                ["transform-es2015-modules-commonjs-simple", {
                    "noMangle": true
                }],
                "transform-object-rest-spread"
            ]
        }
    }
}
